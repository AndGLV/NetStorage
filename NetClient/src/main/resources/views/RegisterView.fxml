<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox alignment="CENTER" maxHeight="50.0" minHeight="50.0" prefHeight="50.0">
            <children>
                <Label alignment="CENTER_RIGHT" maxHeight="25.0" maxWidth="125.0" minHeight="25.0" minWidth="125.0" prefHeight="25.0" prefWidth="125.0" text="Name: " />
                <TextField fx:id="textName" />
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </HBox>
        <HBox alignment="CENTER" maxHeight="50.0" minHeight="50.0" prefHeight="50.0">
            <children>
                <Label alignment="CENTER_RIGHT" maxHeight="25.0" maxWidth="125.0" minHeight="25.0" minWidth="125.0" prefHeight="25.0" prefWidth="125.0" text="Login: " />
                <TextField fx:id="textLogin" />
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </HBox>
        <HBox alignment="CENTER" maxHeight="50.0" minHeight="50.0" prefHeight="50.0">
            <children>
                <Label alignment="CENTER_RIGHT" maxHeight="25.0" maxWidth="125.0" minHeight="25.0" minWidth="125.0" prefHeight="25.0" prefWidth="125.0" text="Password: " />
                <PasswordField fx:id="textPassword" />
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </HBox>
        <HBox alignment="CENTER" maxHeight="50.0" minHeight="50.0" prefHeight="50.0">
            <children>
                <Label alignment="CENTER_RIGHT" maxHeight="25.0" maxWidth="125.0" minHeight="25.0" minWidth="125.0" prefHeight="25.0" prefWidth="125.0" text="Repeat password: " />
                <PasswordField fx:id="textRepeatPass" />
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </HBox>
        <HBox alignment="CENTER" maxHeight="50.0" minHeight="50.0" prefHeight="50.0">
            <children>
                <Label alignment="CENTER_RIGHT" maxHeight="25.0" maxWidth="125.0" minHeight="25.0" minWidth="125.0" prefHeight="25.0" prefWidth="125.0" text="Check word: " />
                <TextField fx:id="textCheckWord" />
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </HBox>
        <HBox alignment="CENTER" maxHeight="50.0" minHeight="50.0" prefHeight="50.0">
            <children>
                <Label alignment="CENTER_RIGHT" maxHeight="25.0" maxWidth="125.0" minHeight="25.0" minWidth="125.0" prefHeight="25.0" prefWidth="125.0" text="Space available: " />
                <TextField fx:id="textSpace" />
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </HBox>
        <HBox alignment="CENTER" maxHeight="50.0" minHeight="50.0" prefHeight="50.0">
            <children>
                <Button fx:id="buttonReg" maxWidth="80.0" minWidth="80.0" prefWidth="80.0" text="Register">
                    <HBox.margin>
                        <Insets right="10.0" />
                    </HBox.margin>
                </Button>
                <Button fx:id="buttonCancel" maxWidth="80.0" minWidth="80.0" mnemonicParsing="false" prefWidth="80.0" text="Cancel">
                    <HBox.margin>
                        <Insets left="10.0" />
                    </HBox.margin>
                </Button>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </HBox>
        <HBox alignment="CENTER" maxHeight="50.0" minHeight="50.0" prefHeight="50.0">
            <children>
                <Label fx:id="labelStatus" />
            </children>
        </HBox>
    </children>
</VBox>
